<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>2.1.0</Version>
    <Authors>nunobaraujo</Authors>
    <Company>nbsoft.pt</Company>
    <Description>MySql Logger Library for .NET Standard</Description>
    <Copyright>© 2018-2020 Nuno Benjamim Salgado Alves de Araújo</Copyright>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/nunobaraujo/NBsoft.Logs.MySql</PackageProjectUrl>
    <PackageIconUrl></PackageIconUrl>
    <PackageTags>log logging mysql</PackageTags>
    <RepositoryUrl>https://github.com/nunobaraujo/NBsoft.Logs.MySql</RepositoryUrl>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageIcon>NB.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MySql.Data" Version="8.0.22" />
    <PackageReference Include="NBsoft.Logs" Version="2.1.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\..\NB.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
